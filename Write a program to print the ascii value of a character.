import java.util.*;
public class Main
{
    public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    System.out.println("Enter the Character: ");
    char ch = sc.next().charAt(0);
    int ascii = ch;
    
    System.out.println("The ASCII value of " + ch + " is: " + ascii);
	}
}
