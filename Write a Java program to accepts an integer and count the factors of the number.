import java.util.*;
public class Main
{
	public static void main(String[] args) {
	Scanner sc = new Scanner(System.in);
	System.out.println("Enter the value: ");
	int a=sc.nextInt();
	
	int count=0;
	for(int i=1;i<=a;i++)
	{
	    if(a%i==0)
	    {
	        count++;
	    }
	}
	System.out.println(count);
	}
}
