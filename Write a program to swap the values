import java.util.Scanner;
public class Main
{
    public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    System.out.println("Enter the first number:");
    int num1 = sc.nextInt();
    System.out.println("Enter the second number:");
    int num2 = sc.nextInt();
    System.out.println("Values before swapping are:");
    System.out.println("First Value before swapping is = "+num1);
    System.out.println("Second Value before swapping is = "+num2);
    int temp=0;
    temp=num1;
    num1=num2;
    num2=temp;
    System.out.println("Values after swapping are:");
    System.out.println("First Value after swapping is = "+num1);
    System.out.println("Second Value after swapping is = "+num2);
	
  }
}
