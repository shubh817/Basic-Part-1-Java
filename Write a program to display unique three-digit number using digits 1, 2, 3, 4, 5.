import java.util.*;
public class Main
{
    public static void main(String[] args) {
    int count=0;
    for(int i=1;i<=5;i++)
    {
        for(int j=1;j<=5;j++)
        {
            for(int k=1;k<=5;k++)
            {
                if(i!=j && j!=k && k!=i)
                count++;
                System.out.println(i+" "+j+" "+k);
            }
        }
    }
    System.out.println("Total different numbers formed with three digits are:"+count);
    
	}
}
